#elseif($templateId.equals("TABLE"))
		<span id="cell_$FORMCELL_ID" class="col-sm-$FORMCELL_WIDTH padding_1" title="$!FORMCELL_TITLE">
			<label class="col-sm-$FORMCELL_LABLEWIDTH control-label no-padding-right">
				$FORMCELL_DESCR
			</label>
			<div class="col-sm-$FORMCELL_LABLEWIDTH_REMAIN"   >
				<table class="table table-bordered table-hover">
                	<thead>
                		<tr>
	                		#foreach($table_formcell in $TABLE_FORMCELLS )
								<th class="center" style="#if($table_formcell.FORMCELL_TYPECODE.equals("INPUTHIDDEN") && $table_formcell.isdesign == false)display: none;#end #if($!table_formcell.FORMCELL_TABLE_COLUMN_WIDTH !="")width: ${table_formcell.FORMCELL_TABLE_COLUMN_WIDTH};#end">
									#if($FORMCELL_REQUIRED.equals("true") and !$table_formcell.FORMCELL_TYPECODE.equals("FILE"))<i class="ace-icon fa fa-asterisk red smaller-70"></i>#end
									#if(!$table_formcell.FORMCELL_TYPECODE.equals("INPUTHIDDEN"))
									$table_formcell.FORMCELL_DESCR
									#end
								</th>
							#end
							<th class="center" style="width:40px;">
								<a href="javascript:void(0);" onclick="jdp_eform_addTableRow(this);">
	                        		<i class="ace-icon fa fa-plus"></i>
	                      		</a>
							</th>
						</tr>
                	</thead>
                	<tbody>
                		<c:choose>
	                		<c:when test='#literal()${#end multirowtable != null && multirowtable["$TABLE_CODE"].row != null && fn:length(multirowtable["$TABLE_CODE"].row) > 0 #literal()}#end'>
	                			<c:forEach var="item" items='#literal()${#end multirowtable["$TABLE_CODE"].row #literal()}#end' varStatus="rowStatus" >
	                				#set($TABLE_ROW_INDEX="rowStatus.index")
	                				#set($multi_table_row="1")
	                				#EXPORT_BASE_TABLE($TABLE_FORMCELLS, $TABLE_ROW_INDEX, $multi_table_row)
	                			</c:forEach>
	                		</c:when>
	                		<c:otherwise>
	                			<c:forEach var="index" begin="1" end="$FORMCELL_TABLE_ROW_INIT_NUM">
	                				#set($TABLE_ROW_INDEX="index - 1")
	                				#set($multi_table_row="")
								  	#EXPORT_BASE_TABLE($TABLE_FORMCELLS, $TABLE_ROW_INDEX)
								</c:forEach>
	                		</c:otherwise>
	                	</c:choose>
                	</tbody>
                </table>
			</div>
		</span>
#elseif($templateId.equals("ROWHTML"))
<div  id="row_$FORMROW_ID"  class="form-group $INITROWCLASS">
	$CONTENT
	#if($isdesign != false)
	<div class="actions-wrapper">
		<a class="js-duplicate fb-button" title="Duplicate Row" onclick="window.parent.doSaveRow('$FORMROW_ID')"><i class="fa fa-plus-circle"></i></a>
		<a class="js-clear fb-button" title="Remove Row" onclick="window.parent.doDeleteRow('$FORMROW_ID')"><i class="fa fa-minus-circle"></i></a>
	</div>
	#end
</div>
##表单控件关系
#elseif($templateId.equals("RELATION_SCRIPT"))
	<script>
		$(function(){
			##绑定事件
			#foreach($relation in $relationMap.entrySet())
				$("#$relation.key").bind('input propertychange', function() {
					#foreach($expression in $relation.value)
						$('#$expression.split("=")[1]').val($expression.split("=")[0]);
					#end
				});
			#end
			//IE9 back键触发不了input事件
			if(navigator && navigator.appName == "Microsoft Internet Explorer" && navigator.appVersion.split(";")[1].replace(/[ ]/g,"")=="MSIE9.0"){
				#foreach($relation in $relationMap.entrySet())
					$("#$relation.key").keyup(function() {
						#foreach($expression in $relation.value)
							$('#$expression.split("=")[1]').val($expression.split("=")[0]);
						#end
					});
				#end
			}
		});
	</script>
#end